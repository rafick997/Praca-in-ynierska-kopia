@model IEnumerable<Przychodnia_medyczna.Models.TestGroup>

@{
    ViewBag.Title = "Lista rodzajów badań";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Lista rodzajów</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GroupName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.GroupName)
        </td>
        <td>
            @Html.ActionLink("Edytuj", "Edit", new { id=item.TestGroupId }) |
            @Html.ActionLink("Usuń", "Delete", new { id=item.TestGroupId })
        </td>
    </tr>
}

</table>
